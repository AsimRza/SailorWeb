@model IEnumerable<SailorWeb.Models.Model.Contact_TBL>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="header bg-primary pb-6">
    <div class="container-fluid">
        <div class="header-body">
            <div class="row align-items-center py-4">
                <div class="col-lg-6 col-7">
                    <h6 class="h2 text-white d-inline-block mb-0">Əlaqə</h6>
                    <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
                        <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
                            <li class="breadcrumb-item"><a href="/Home/Index"><i class="fas fa-home"></i></a></li>
                            <li class="breadcrumb-item"><a href="/Contact/Index">Əlaqə</a></li>
                      
                        </ol>
                    </nav>
                </div>
              
            </div>
        </div>
    </div>
</div>
<!-- Page content -->
<div class="container-fluid mt--6">
    <div class="row">
        <div class="col">
            <div class="card">
                <!-- Card header -->
                <div class="card-header border-0">
                    <h3 class="mb-0">Məlumatlar</h3>
                </div>
                <!-- Light table -->
                <div class="table-responsive" style=" min-height: 300px !important;">
                    <table class="table align-items-center table-flush">
                        <thead class="thead-light">
                            <tr>
                                <th scope="col" class="sort">Ərazi</th>
                                <th scope="col" class="sort">Email</th>
                                <th scope="col" class="sort">Nömrə</th>
                                <th scope="col">Twitter</th>
                                <th scope="col">Facebook</th>
                                <th scope="col">Instagram</th>
                                <th scope="col">Skype</th>
                                <th scope="col">LinkedIn</th>
                                <th scope="col"></th>
                            </tr>
                        </thead>
                        <tbody class="list">
                          
                               @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @item.Location
                                        </td>
                                        <td>
                                            @item.Email
                                        </td>
                                        <td>
                                            @item.Number
                                        </td>
                                        <td>
                                            @item.Twitter
                                        </td>
                                        <td>
                                            @item.Facebook
                                        </td>
                                        <td>
                                            @item.Instagram
                                        </td>
                                        <td>
                                            @item.Skype
                                        </td>
                                        <td>
                                            @item.Linkedin
                                        </td>
                                        <td class="text-right">
                                            <div class="dropdown">
                                                <a class="btn btn-sm btn-icon-only text-light" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <i class="fas fa-ellipsis-v"></i>
                                                </a>
                                                <div class="dropdown-menu dropdown-menu-right dropdown-menu-arrow">
                                                    <a class="dropdown-item" href="/Contact/Details/@item.ContactId"><i class="fas fa-eye text-info"></i>Ətraflı Bax</a>
                                                    <a class="dropdown-item" href="/Contact/Edit/@item.ContactId"><i class="fas fa-sync-alt text-primary"></i>Redaktə et</a>

                                                </div>
                                            </div>
                                        </td>
                                    </tr>
                                }
                            



                        </tbody>
                    </table>
                </div>
                <!-- Card footer -->
      @*          <div class="card-footer py-4">
                    <nav aria-label="...">
                        <ul class="pagination justify-content-end mb-0">
                            <li class="page-item disabled">
                                <a class="page-link" href="#" tabindex="-1">
                                    <i class="fas fa-angle-left"></i>
                                    <span class="sr-only">Previous</span>
                                </a>
                            </li>
                            <li class="page-item active">
                                <a class="page-link" href="#">1</a>
                            </li>
                      
                            <li class="page-item">
                                <a class="page-link" href="#">
                                    <i class="fas fa-angle-right"></i>
                                    <span class="sr-only">Next</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>*@
            </div>
        </div>
    </div>


